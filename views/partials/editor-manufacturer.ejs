<div class="form-group row mb-3">
  <label class="col-form-label col-sm-2" for="name">제조사</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="name" id="name" value="<%= model.name %>"/>
    </div>
</div>
<div class="form-group row mb-3">
  <label for="postcode" class="col-form-label col-sm-2">우편번호</label>
  <div class="col-sm-8">
    <input class="form-control" name="postcode" id="postcode" value="<%= model.postcode %>">
  </div>
</div>
<div class="form-group row mb-3">
  <label class="col-form-label col-sm-2" for="address">주소</label>
  <div class="col-sm-8">
    <input class="form-control" name="address" id="address" value="<%= model.address %>"/>
  </div>
</div>
<div class="form-group row mb-3">
  <label class="col-form-label col-sm-2" for="detailAddress">상세주소</label>
  <div class="col-sm-8">
    <input class="form-control" name="detailAddress" id="detailAddress" value="<%= model.detail_address %>"/>
  </div>
</div>
<div class="form-group row mb-3">
  <label for="production" class="col-form-label col-sm-2">생산량</label>
  <div class="col-sm-8">
    <input class="form-control" name="production" id="production" value="<%= model.production %>"/>
  </div>
</div>
<div class="form-group row mb-3">
  <label for="approved" class="col-form-label col-sm-2">승인</label>
  <div class="col-sm-8">
    <input readonly class="form-control" name="approved" id="approved" value="<%= model.approved %>"/>
  </div>
</div>

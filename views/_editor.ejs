    <div class="col-sm-8">
      <input autofocus class="form-control" name="name" value="<%= model.name %>"/>
    </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="address">주소</label>
  <div class="col-sm-7">
    <input class="form-control" name="address" value="<%= model.address %>"/>
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="brand">김치 브랜드</label>
  <div class="col-sm-10">
    <input class="form-control" name="brand" value="<%= model.brand %>"/>
  </div>
</div>